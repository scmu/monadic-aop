%format succeq = "(\succeq)"
%format `succeq` = "\succeq"

%format leqs = "(\leq_{s})"
%format `leqs` = "\leq_{s}"
%format geqs = "(\geq_{s})"
%format `geqs` = "\geq_{s}"
%format leqv = "(\leq_{v})"
%format `leqv` = "\leq_{v}"
%format geqv = "(\geq_{v})"
%format `geqv` = "\geq_{v}"
%format leqvw = "(\leq_{\mathit{vw}})"
